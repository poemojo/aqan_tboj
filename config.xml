<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aqan.tboj" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>A Quest About Nothing: The Blade of Justice</name>
    <description>
        An interactive story adventure following the hard gritted bounty hunter, Amabo Karrab's, quest for the rogue vigilante The Blade of Justice. 
    </description>
    <author email="david.levinson@poemojo.com" href="http://davidplevinson.com">
        David Levinson,
        Tim Krajewski,
        Chris Stehm
    </author>
   

    <preference name="permissions" value="none" />
    <preference name="target-device" value="universal" />
    <oreference name="webviewbounce" value="false" />
    <preference name="stay-in-webview" value="false" />
    <content src="index.html" />
    <access origin="*" />
   <plugin name="cordova-plugin-inappbrowser" />
    <plugin name="cordova-plugin-whitelist" />
    <allow-navigation href="*" />
</widget>
